<!-- @@NO_DATA@@ -->
<div class="no-data">
	No records were found for your selection. Please modify your filters.
</div>
<!-- @@NO_DATA@@ -->
<!-- @@TAB@@ -->
<h3>Settlement Files</h3>
<table id="files-table">
	<thead>
		<tr>
			<th class="left">Settlement File Name</th>
			<th class="right">Sent Date</th>
			<th class="left">Acknowledgement File Name</th>
			<th class="right">Settlement Date</th>
		</tr>
	</thead>
	<tbody>
		<!-- @@ROW@@ -->
		<tr data-file-id="@@SettlementFileId@@" data-file-name="@@SettlementFileName@@">
			<td>@@SettlementFileName@@</td>
			<td class="right">@@SettlementFileDate@@</td>
			<td>@@AcknowledgementFileName@@</td>
			<td class="right">@@AcknowledgementFileDate@@</td>
		</tr>
		<!-- @@ROW@@ -->
	</tbody>
</table>
<!-- @@TAB@@ -->
