<!-- @@NO_DATA@@ -->
<div class="no-data">
	No records were found for your selection. Please modify your filters.
</div>
<!-- @@NO_DATA@@ -->
<!-- @@TAB@@ -->
<h3>File Transactions</h3>
<table id="transactions-table">
	<thead>
		<tr class="right">
			<th>Record No</th>
			<th>Transaction Id</th>
			<th>Charge Id</th>
			<th>Refund Id</th>
			<th>Created Date</th>
			<th>Amount</th>
			<th>State</th>
			<th>Status</th>
		</tr>
	</thead>
	<tbody>
		<!-- @@ROW@@ -->
		<tr class="right" data-transaction-id="@@Id@@">
			<td>@@RecordNo@@</td>
			<td>@@Id@@</td>
			<td>@@ChargeId@@</td>
			<td>@@RefundId@@</td>
			<td>@@CreatedDate@@</td>
			<td>@@Amount@@</td>
			<td>@@State@@</td>
			<td>@@Status@@</td>
		</tr>
		<!-- @@ROW@@ -->
	</tbody>
</table>
<!-- @@TAB@@ -->
